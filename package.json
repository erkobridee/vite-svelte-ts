{
  "name": "vite-svelte-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prettier": "prettier --write .",
    "eslint": "eslint --fix \"src/**/*.{js,ts,svelte}\"",
    "lint": "run-s prettier eslint",
    "start": "vite",
